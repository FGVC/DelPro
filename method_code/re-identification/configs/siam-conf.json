{
    "description": "Amur tiger siamese metric learning.",
    "network": {
        "module": "models",
        "model": "ResnetEmbed",
        "kwargs": {
            "model": "resnet18",
            "pretrained": true,
            "gray": true
        }
    },
    "dataset": {
        "module": "dataset",
        "dataset": "ReIDDataset",
        "kwargs": {
            "split": "reid-hard-splits.split1",
            "color": "L"
        }
    },
    "loss": {
        "module": "utils",
        "loss": "AllPairContrastLoss",
        "kwargs": {
            "mn": 0.5,
            "mp": 0.25
        }
    },
    "trainer": {
        "module": "utils",
        "trainer": "SiameseMetricTrainer"
    },
    "optimizer": {
        "module": "torch.optim",
        "optim": "SGD",
        "kwargs": {
            "lr": 5e-2,
            "weight_decay": 1e-5,
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "hyperparams": {
        "epochs": 12,
        "lr": 5e-2,
        "batch_size": 40,
        "num_workers": 8
    },
    "other": {

    }
}
